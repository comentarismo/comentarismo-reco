{
  "name": "Comentarismo-Reco",
  "version": "0.0.0",
  "description": "Comentarismo Recomendation Engine API",
  "main": "comentarismoger.js",
  "scripts": {
    "docs": "node_modules/.bin/docco server/*.coffee test/**/*.coffee public/js/main.js",
    "build": "node_modules/.bin/coffee --no-header --map --output dist -c server/*.coffee",
    "postinstall": "npm run build",
    "start-prod": "node dist",
    "start": "node_modules/.bin/nodemon ./server/index.coffee",
    "test": "mocha",
    "test-dev": "DEBUG= RETHINKDB_HOST=g7-box RETHINKDB_DB=hapiger_it mocha",
    "coverage": "npm run coverage:rm && npm run coverage:cover && npm run coverage:report && npm run coverage:check",
    "coverage:cover": "RETHINKDB_HOST=g7-box node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- test -R spec",
    "coverage:rm": "rm -rf coverage",
    "coverage:report": "node_modules/.bin/istanbul report",
    "coverage:check": "node_modules/.bin/istanbul check-coverage ./coverage/coverage.json",
    "coverage:esms":"node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- test/esms/ -R spec"
  },
  "dependencies": {
    "basic-auth-connect": "1.0.0",
    "bluebird": "3.4.7",
    "body-parser": "1.16.0",
    "catbox-redis": "2.0.2",
    "coffee-script": "1.12.3",
    "compression": "1.6.2",
    "docco": "0.7.0",
    "express": "^4.14.0",
    "good": "7.1.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.1",
    "hapi": "16.1.0",
    "hapigerjs": "^1.0.1",
    "inert": "4.1.0",
    "jade": "~1.1.5",
    "joi": "10.2.2",
    "lodash": "3.9.3",
    "moment": "2.17.1",
    "morgan": "1.7.0",
    "nedb": "^1.8.0",
    "nstore": "~0.5.2",
    "rethinkdbdash": "2.3.27",
    "vision": "4.1.1"
  },
  "devDependencies": {
    "browser-sync": "2.18.8",
    "chai": "3.5.0",
    "coffee-coverage": "1.0.1",
    "gulp": "3.9.1",
    "gulp-npm-run": "0.0.3",
    "gulp-shell": "0.5.2",
    "gulp-watch": "4.3.11",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "nodemon": "1.11.0"
  }
}
